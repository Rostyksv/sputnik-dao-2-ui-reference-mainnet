ze=\"sm\" disabled={currentPage === 0} onClick={onPrevious}>\n            <MDBIcon fas icon=\"angle-left\" />\n          </MDBBtn>\n          {paginationRange.map((pageNumber, i) => {\n            if (pageNumber === DOTS) {\n              return (\n                <MDBBtn size=\"sm\" color=\"elegant\" i={`dot-key-${i}`}>\n                  &#8230;\n                </MDBBtn>\n              );\n            }\n\n            return (\n              <MDBBtn\n                key={`page-key-${pageNumber}`}\n                color={pageNumber === currentPage + 1 ? 'dark' : 'elegant'}\n                size=\"sm\"\n                selected={pageNumber === currentPage + 1}\n                onClick={() => onPageChange(pageNumber - 1)}\n              >\n                {pageNumber}\n              </MDBBtn>\n            );\n          })}\n          <MDBBtn\n            size=\"sm\"\n            color=\"elegant\"\n            disabled={currentPage + 1 === lastPage}\n            onClick={onNext}\n          >\n            <MDBIcon fas icon=\"angle-right\" />\n          </MDBBtn>\n        </div>\n      ) : null}\n    </>\n  );\n};\n\nexport default Pagination;\n"},"lineCount":null}},"error":null,"hash":"0f8c4fd8dee6f5c0d7dcc4d2600b4255","cacheData":{"env":{}}}